module MapHelper

  def get_state_name_from_code(st)
    for s in US_STATES
      if st == s[1]
        return s[0]
      end
    end
    return nil
  end

  def get_state_code_from_name(st)
    for s in US_STATES
      if st == s[0]
        return s[1]
      end
    end
  end

  def get_map_name(st)

    if st == "USA"
      return "FCMap_USA.swf"
    else
      for z in US_STATES
        if z[1] == st
          return "FCMap_#{z[0]}.swf"
        end
      end
    end

  end

