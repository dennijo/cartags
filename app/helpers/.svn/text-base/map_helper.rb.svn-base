  def get_state_code_array

    transposed = US_STATES.transpose
    return transposed[1]

  end

  def get_county_code_array(st)

    array = get_array(st)
    transposed = array.transpose
    # third column contains the actual county names
    return transposed[2]

  end

  # st = State, default = "USA"
  # hits = entries
  def get_map_xml(st,hits,legend=1)

    data = "<map showCanvasBorder='0' showLegend='#{legend}' borderColor='FFFFFF' fillColor='CCCCCC' numberSuffix=' Tags' includeValueInLabels='1' labelSepChar=': ' bgAlpha='0'  showLabels='0' baseFontSize='9' showToolTip='1'>"
    data << "<colorRange>"
    data << "<color minValue='1' maxValue='20' displayValue='Few Tags' color='A7E9BC' />"
    data << "<color minValue='20' maxValue='100' displayValue='Moderate Tags' color='FFFFCC' />"
    data << "<color minValue='100' maxValue='100000' displayValue='High Tags' color='FF9377' />"
    data << "</colorRange>"
    data << "<data>"
    if st == "USA"
      for s in US_STATES
        # very inefficient!!!!!
        num_hits = 0
        for hit in hits
          if hit.state == s[1]
            num_hits = hit.hits
          end
        end
        data << "<entity id='#{s[1]}' toolText='#{s[0]}: #{num_hits} tags' link='/search/map/?st=#{s[1]}%26amp%3Bname=#{s[0]}' value='#{num_hits}'/>"
      end
    else
      array = get_array(st)
      for c in array

        num_hits = 0
        for hit in hits
          if hit.county == c[2].strip
            num_hits = hit.hits
          end
        end

        data << "<entity id='#{c[0].strip}' toolText='#{c[2]}: #{num_hits} hits' link='/search/map/?st=#{st}%26amp%3Bcounty=#{fix_county_name(c[2])}' value='#{num_hits}'/>"
      end
    end
    data << "</data></map>"

    puts "#{data}\n\n"

    return data
  end

  def fix_county_name(county_name)
    county_name.gsub(/\s/,'%20').strip
  end

